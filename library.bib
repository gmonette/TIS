@misc{BBC2016,
abstract = {A Dutch statistician recently became suspicious by headlines in the Dutch news that women were being discriminated against when it came to getting science research funding. Professor Casper Albers of the Heymans Institute for Psychological Research, Groningen, discovered that the study into the funding process showed that when you looked at the overall numbers of successful candidates, women seemed to be less successful than men. And yet, when you looked at a breakdown of the different subjects people could apply for, it showed that women were not losing out disproportionately to men. How could two opposite findings be true? This contradiction is explained by a famous statistical paradox. We explain what is known as Simpson's Paradox with the aid of a choir metaphor, performed by the BBC Singers.},
author = {{(BBC World Service)}},
file = {:C$\backslash$:/Users/georges/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/4dd0e28415f9fbe2cdf3d4c1c562c26657d5d22f.html:html},
pages = {Monday, May 2, 2016 10:15},
title = {{Simpson's Paradox}},
url = {http://www.bbc.co.uk/programmes/p03rsqkc},
year = {2016}
}
