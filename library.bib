@article{Reed2012,
abstract = {Introduction/Objectives: Mild traumatic brain injury (mTBI), or concussions, are common during youth hockey participation, in which persisting functional deficits can result. In 2009-10, over 500,000 Canadian youth between the ages of 5 to 17 years were registered to play hockey, leaving many youth athletes at risk for sustaining a mTBI. Despite growing public interest in sport-related mTBI, what remains to be explored is the influence of mTBI on strength performance in youth. It is possible that deficits in strength post mTBI may put youth hockey players at risk for decreased performance and re-injury upon returning to play. The purpose of this study was to explore the influence of age, gender and mTBI on strength performance within youth hockey players. Methods: This study used a three-year prospective longitudinal research design. The participant group was made up of 178 unique male and female participants (ages 8 to 14 years). Nineteen of the 178 participants sustained a mTBI while enrolled in the study, where three participants sustained repeated mTBIs for a total of 23 concussive events. Baseline and post-mTBI data on leg maximal voluntary contraction, jump test and hand grip performance were collected. Results: Using a linear mixed-effects model, interaction and linear models reporting effects of age, gender and sustaining a mTBI on strength performance were explored. Data specific to the full participant sample, as well as the mTBI subset are reported. When accounting for severity of postconcussive symptoms, significant average effects were found for jump height (squat jump: Estimate=-0.05; SE=0.02; t=-2.51; P=0.0186; countermovement jump: Estimate=-0.03; SE=0.02; t=-2.20; P=0.0371) during the symptomatic stage post-mTBI and for leg maximal voluntary contraction (Estimate=-1.05; SE=0.47; t=-2.27; P=0.0421) during the asymptomatic stage post-mTBI, indicating decreased strength performance following mTBI. Conclusions: This study acts as an initial step towards better understanding mTBI-related strength performance deficits that may limit the on and off ice occupational performance of the youth hockey player population and in turn, can act as a foundation for future youth, adolescent and adult sportrelated mTBI research and rehabilitation protocols.},
author = {Reed, N and Taha, T and Tremblay, L and Monette, G and Keightley, M},
journal = {Brain Injury},
keywords = {adolescent,adult,athlete,brain injury,concussion,female,gender,hand grip,height,human,ice hockey,injury,juvenile,leg,male,methodology,model,non profit organization,population,rehabilitation,risk,sport,statistical model,traumatic brain injury},
number = {4-5},
pages = {766},
title = {{Mild traumatic brain injury and strength performance in youth ice hockey players}},
