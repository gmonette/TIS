doi = {10.1002/sim.3843},
file = {:C$\backslash$:/Users/georges/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Burgess et al. - 2010 - Bayesian methods for meta-analysis of causal relationships estimated using genetic instrumental variables.pdf:pdf},
